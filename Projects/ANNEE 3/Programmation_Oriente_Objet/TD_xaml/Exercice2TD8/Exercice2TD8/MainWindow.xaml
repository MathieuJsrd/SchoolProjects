<Window x:Class="Exercice2TD8.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Exercice2TD8"
        mc:Ignorable="d"
        Title="MainWindow" Height="100" Width="300">
    <StackPanel Name="Panel1" Orientation="Horizontal">
        <TextBox Name="TextDeGauche" Width="80" Height="30" Margin="0,0,10,0"></TextBox>
        <StackPanel Name="Panel2" Orientation="Vertical">
            <Button Name="BoutonHaut" Height="30" Width="50" Content="==&gt;" Click="ClickBoutonHaut" ClickMode="Press"></Button>
            <Button Name="BoutonBas" Height="30" Width="50" Content="&lt;==" Click="ClickBoutonBas" ClickMode="Press"></Button>
        </StackPanel>
        <TextBox Name="TextDeDroite" Width="80" Height="30" Margin="10,0,0,0"></TextBox>
    </StackPanel>
</Window>
